{
  "version": 2,
  "buildCommand": "cd next-app && npm install && npm run build",
  "outputDirectory": "next-app/.next",
  "installCommand": "cd next-app && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/next-app/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "",
    "POSTGRES_URL": "",
    "API_KEY": "",
    "NEXTAUTH_URL": "",
    "NEXTAUTH_SECRET": "",
    "SENTRY_DSN": ""
  }
}
